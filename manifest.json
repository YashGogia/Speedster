{
  "name": "Speedster",
  "version": "1.0.0",
  "icons": {
      "128" : "flash.png"
  } ,
  "description": "Control HTML5 video speeds with keyboard shotcuts",
  "permissions": ["tabs"],
  "manifest_version": 2,
  "content_scripts": [
    {
      "all_frames": true,
      "run_at": "document_start",
      "matches": ["<all_urls>"],
      "js": ["jquery-2.2.4.min.js", "mutation-summary.js", "speedster.js"]
    }
  ]
}
